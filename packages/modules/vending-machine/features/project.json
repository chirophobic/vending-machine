{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "name": "modules-vending-machine-features",
  "sourceRoot": "packages/modules/vending-machine/features/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/modules/vending-machine/features",
        "tsConfig": "packages/modules/vending-machine/features/tsconfig.lib.json",
        "packageJson": "packages/modules/vending-machine/features/package.json",
        "main": "packages/modules/vending-machine/features/src/index.ts",
        "assets": ["packages/modules/vending-machine/features/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/modules/vending-machine/features/**/*.ts"]
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["dist/packages/modules/vending-machine/features/cucumber-report.html"],
      "options": {
        "command": "NODE_OPTIONS=\"--require ./tools/import-overrides.js --require source-map-support/register\" yarn cucumber-js --config packages/modules/vending-machine/features/cucumber.config.json"
      },
      "dependsOn": ["build"]
    }
  },
  "tags": []
}
